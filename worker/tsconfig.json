{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"noEmit": true,
		"lib": ["ES2023"],
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["src"]
}
